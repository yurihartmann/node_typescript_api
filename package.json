  "bugs": {
    "url": "https://github.com/yurihartmann/node_typescript_api/issues"
  },
  "homepage": "https://github.com/yurihartmann/node_typescript_api#readme",
  "dependencies": {
    "module-alias": "^2.2.2"
  },
  "devDependencies": {
    "@types/jest": "^27.4.0",
    "@types/module-alias": "^2.0.1",
    "@types/node": "^17.0.10",
    "@types/supertest": "^2.0.11",
    "@typescript-eslint/eslint-plugin": "^5.10.0",
    "@typescript-eslint/parser": "^5.10.0",
    "eslint": "^8.7.0",
    "jest": "^27.4.7",
    "supertest": "^6.2.2",
    "ts-jest": "^27.1.3",
